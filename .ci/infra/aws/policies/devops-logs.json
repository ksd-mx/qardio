{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "newsfeed-AllowCloudWatchLogging",
      "Effect": "Allow",
      "Action": [
        "logs:CreateLogGroup",
        "logs:DeleteLogGroup",
        "logs:DescribeLogGroups",
        "logs:ListTagsLogGroup",
        "logs:TagLogGroup"
      ],
      "Resource": "*"
    }
  ]
}
